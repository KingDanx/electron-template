{
  "name": "electron-template",
  "productName": "electron-template",
  "version": "1.0.2",
  "description": "My Electron application description",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"cd src/frontend && npm run dev\" \"electron-forge start\"",
    "package": "cd src/frontend && npm run build && cd ../.. &&  electron-forge package",
    "make": "cd src/frontend && npm run build && cd ../.. && electron-forge make",
    "publish": "electron-forge publish",
    "install:all": "cd src/frontend && npm install && cd ../.. && npm install",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Dan Prudhomme Jr",
    "email": "dannyprud@yahoo.com"
  },
  "license": "MIT",
  "dependencies": {
    "@kingdanx/litelogger": "^1.1.0",
    "concurrently": "^9.2.0",
    "electron-is-dev": "^3.0.1",
    "electron-squirrel-startup": "^1.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "electron": "37.3.1"
  }
}